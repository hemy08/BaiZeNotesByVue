{
  "name": "HemyMarkdownEditor",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "hemy08",
  "email": "zhaojunwei008@yeah.net",
  "repo": "https://github.com/hemy08/HemyMarkdownEditor",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@electron/remote": "^2.1.2",
    "@types/jsdom": "^21.1.7",
    "electron-store": "^9.0.0",
    "electron-updater": "^6.1.8",
    "highlight.js": "^11.9.0",
    "jsdom": "^24.1.0",
    "katex": "^0.16.10",
    "markdown-it-anchor": "^9.0.1",
    "markdown-it-highlightjs": "^4.1.0",
    "markdown-it-plantuml": "^1.4.1",
    "markdown-it-prism": "^2.3.0",
    "markdown-it-textual-uml": "^0.17.1",
    "md-it-mermaid": "^1.0.0",
    "mermaid": "^10.9.1",
    "plantuml-cli": "^1.2024.5",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "commonmark": "^0.31.0",
    "electron": "^30.0.8",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.26.0",
    "markdown-it": "^14.1.0",
    "markdown-it-mermaid": "^0.2.5",
    "markdown-it-mermaid-less": "^0.0.7",
    "markdown-it-mermaid-plugin": "^0.1.0",
    "markdown-it-textual-uml": "^0.17.1",
    "marked": "^12.0.2",
    "mermaid": "^10.9.1",
    "monaco": "^1.201704190613.0",
    "monaco-editor": "^0.49.0",
    "monaco-themes": "^0.4.4",
    "prettier": "^3.2.5",
    "remarkable": "^2.0.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vue": "^3.4.27",
    "vue-tsc": "^2.0.19"
  }
}
